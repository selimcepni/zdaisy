<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <ViewSettingsDialog confirm="onConfirmFilter">
        <filterItems>
            <ViewSettingsCustomItem text="Yıl">
                <customControl>
                    <DatePicker
                        minDate="{01.01.2023}"
                        id="idBegDatePicker"
                        displayFormat="yyyy"
                        class="sapUiSmallMarginBottom"
                        valueFormat="yyyy"
                    />
                </customControl>
            </ViewSettingsCustomItem>
            <ViewSettingsCustomItem text="Dönem">
                <customControl>
                    <Select
                        editable="true"
                        enabled="true"
                        forceSelection="true"
                        id="idPeriodSelect"
                    >
                        <items>
                            <core:Item
                                key="01"
                                text="01"
                            />
                            <core:Item
                                key="02"
                                text="02"
                            />
                            <core:Item
                                key="03"
                                text="03"
                            />
                            <core:Item
                                key="04"
                                text="04"
                            />
                            <core:Item
                                key="05"
                                text="05"
                            />
                            <core:Item
                                key="06"
                                text="06"
                            />
                            <core:Item
                                key="07"
                                text="07"
                            />
                            <core:Item
                                key="08"
                                text="08"
                            />
                            <core:Item
                                key="09"
                                text="09"
                            />
                            <core:Item
                                key="10"
                                text="10"
                            />
                            <core:Item
                                key="11"
                                text="11"
                            />
                            <core:Item
                                key="12"
                                text="12"
                            />
                        </items>
                    </Select>
                </customControl>
            </ViewSettingsCustomItem>
        </filterItems>
    </ViewSettingsDialog>
</core:FragmentDefinition>